{
  "manifest_version": 3,
  "name": "Highlight All",
  "version": "0.41",
  "description": "Double click a word to highlight all occurrences.",
  "icons": {
    "16": "hl_16.png",
    "32": "hl_32.png",
    "48": "hl_48.png",
    "128": "hl_128.png"
  },
  "action": {
    "default_icon": "hl_16.png",
    "default_title": "Highlight All"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["select.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "permissions": [
    "contextMenus",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "update_url": "https://chrome.cardservices.no/extensions/highlight-all/updates.xml",
  "key": ""
}
